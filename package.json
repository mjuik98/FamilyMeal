{
  "name": "family-meal-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --webpack",
    "build": "next build --webpack",
    "start": "next start",
    "test:smoke": "node scripts/smoke-test.mjs",
    "test:smoke:qa-token-required": "node scripts/smoke-qa-token-required.mjs",
    "test:ui": "node --test tests/ui-theme.test.mjs",
    "test:api": "node --test tests/api-security.test.mjs",
    "test:e2e": "playwright test",
    "test:e2e:install": "playwright install --with-deps chromium",
    "audit:report": "npm audit --json",
    "audit:prod:check": "node scripts/audit-prod-check.mjs",
    "ci:verify": "npm run lint && npm run typecheck && npm run build && npm run test:smoke && npm run test:ui && npm run test:api && npm run test:rules",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test:rules": "npx firebase-tools emulators:exec --only firestore \"node --test tests/firestore.rules.test.mjs\"",
    "migrate:comments": "node scripts/migrate-comments-to-subcollection.mjs",
    "migrate:comments:dry": "node scripts/migrate-comments-to-subcollection.mjs --dry-run",
    "migrate:meals": "node scripts/migrate-meals-schema.mjs",
    "migrate:meals:dry": "node scripts/migrate-meals-schema.mjs --dry-run"
  },
  "dependencies": {
    "@ducanh2912/next-pwa": "^10.2.9",
    "@opentelemetry/api": "^1.9.0",
    "@upstash/ratelimit": "^2.0.8",
    "@upstash/redis": "^1.36.2",
    "firebase": "^12.9.0",
    "lucide-react": "^0.575.0",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-calendar": "^6.0.0",
    "react-dom": "19.2.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "^5.0.0",
    "@playwright/test": "^1.56.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.6",
    "firebase-admin": "^13.6.1",
    "typescript": "^5"
  }
}
